{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-ruby-docs",
  "title": "Search Ruby Docs",
  "description": "Search Ruby docs for a specific Ruby version",
  "icon": "ruby-icon.png",
  "author": "CarlGustafsson",
  "categories": [
    "Developer Tools",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Ruby Documentation",
      "subtitle": "Search Ruby Documentation",
      "description": "Show search results from Ruby documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "@raycast/utils": "^1.4.8",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
